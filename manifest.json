{
  "manifest_version": 3,
  "name": "Bale Extension",
  "version": "1.0.0",
  "version_name": "1.0 beta",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "badge.png",
    "48": "badge.png",
    "128": "badge.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://web.bale.ai/*" ],
      "resources": [ "js/core.js","css/main.css"],
      "js": ["js/core.js"],
      "css": ["css/main.css"]
    }
  ]
}